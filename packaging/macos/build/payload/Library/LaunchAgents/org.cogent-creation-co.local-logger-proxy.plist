<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.cogent-creation-co.local-logger-proxy</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/local-logger</string>
        <string>proxy</string>
        <string>--port</string>
        <string>6969</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/local-logger-proxy.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/local-logger-proxy.err</string>

    <key>ProcessType</key>
    <string>Background</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
