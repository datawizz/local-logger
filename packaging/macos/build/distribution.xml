<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="2">
    <title>Local Logger</title>
    <welcome file="welcome.html"/>
    <conclusion file="conclusion.html"/>
    <pkg-ref id="org.local-logger.pkg"/>
    <options customize="never" require-scripts="false" hostArchitectures="arm64,x86_64"/>
    <choices-outline>
        <line choice="default">
            <line choice="org.local-logger.pkg"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="org.local-logger.pkg" visible="false">
        <pkg-ref id="org.local-logger.pkg"/>
    </choice>
    <pkg-ref id="org.local-logger.pkg" version="0.1.0" onConclusion="none">local-logger-component.pkg</pkg-ref>
</installer-gui-script>
